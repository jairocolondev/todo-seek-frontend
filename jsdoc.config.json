{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src/app", "src/components", "store"],
    "includePattern": ".jsx?$",
    "excludePattern": "(node_modules/|__tests__/)"
  },
  "plugins": [],
  "opts": {
    "encoding": "utf8",
    "destination": "./docs",
    "recurse": true,
    "verbose": true
  }
}
